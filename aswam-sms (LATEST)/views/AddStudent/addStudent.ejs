                        <div class="col-md-12">
                                Date of Birth   <input type="date" name="Date_of_Birth" class="form-control" id="dob">
                        </div>
                        <div class="col-md-12">
                                Gender   
                                <div>
                                        Male <input type="radio" name="Gender" value="Male"  id="Gender"> female <input type="radio" name="Gender" value="female"  id="Gender">
                                </div>
                        </div>
                        <div class="col-md-12">
                                Blood group <input type="text" name="Blood_group" id="bloodgroup" class="form-control">
