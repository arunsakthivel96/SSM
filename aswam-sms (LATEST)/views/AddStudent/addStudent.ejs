<% include ../Elements/sidenav %>    
        <div class="content-i">
                <div class="content-box">
                    <div class="row">
                        <div class="col-sm-12">
        <form action="/addmission/save" method="post">
        <div class="row">
                <div class="col-sm-2"></div>
                <div class="col-sm-8" style="background-color:#fff; padding: 20px">
                    <div class="col-md-12">
                        <div class="col-md-12">
                                <div>
                                    Addmission No <input type="text" name="Admission_number" class="form-control" id="AddmissionNo">
                                </div>                               
                        </div>
                        <div class="col-md-12">
                                <div>
                                Addmission Date <input type="date" name="Admission_Date" class="form-control" id="AddmissionNo">
                                </div> 
                        </div>
                        <div class="col-md-12">
                                <label for="">Personal details</label>
                                <div style="  height: 3px;width: 100%;background-color: brown;"></div>
                        </div>
                        <div class="col-md-12">
                            <div>
                            First Name <input type="text" class="form-control" name="First_Name" id="fname">
